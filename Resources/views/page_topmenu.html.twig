<div class="navbar-inner">
    <div class="container">

        <a class="btn btn-navbar" data-action="toggleclass" data-class="nav-collapse" data-target=".main-navi .nav-collapse">{% trans %}Navigation{% endtrans %}</a>

        <div class="nav-collapse">
            <ul class="nav">
            {% for location in locationList %}
                <li id="nav-location-{{ location.id }}"><a href="{{ path( location ) }}">{{ location.contentInfo.name }}</a></li>
            {% endfor %}
            </ul>
        </div>

    </div>
</div>
